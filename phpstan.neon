includes:
    - vendor/saschaegerer/phpstan-typo3/extension.neon

parameters:
  level: 5

  paths:
    - ext_emconf.php
    - ext_localconf.php
    - Classes
    - Tests

  bootstrapFiles:
    - Tests/StaticAnalysis/constants.php

  ignoreErrors:
    - message: '#Variable \$_EXTKEY might not be defined#'
      path: '*/ext_emconf.php'

  reportUnmatchedIgnoredErrors: false
